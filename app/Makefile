	-$(MAKE) -C mem_view all
	-$(MAKE) -C val_view all
	-$(MAKE) -C db all
	-$(MAKE) -C map all
	-$(MAKE) -C voice all
	-$(MAKE) -C propulsion all
	-$(MAKE) -C clock_view all
	-$(MAKE) -C clock_ctrl all
	-$(MAKE) -C http_server all
	-$(MAKE) -C print_meas all
	-$(MAKE) -C xbee all
	-$(MAKE) -C json_convert all

