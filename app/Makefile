	-$(MAKE) -C log_view clean
	-$(MAKE) -C mem_view clean
	-$(MAKE) -C val_view clean
	-$(MAKE) -C db clean
	-$(MAKE) -C map clean
	-$(MAKE) -C voice clean
	-$(MAKE) -C propulsion clean
	-$(MAKE) -C clock_view clean
	-$(MAKE) -C clock_ctrl clean
	-$(MAKE) -C http_server clean
	-$(MAKE) -C print_meas clean
	-$(MAKE) -C xbee clean 
	-$(MAKE) -C json_convert clean
	